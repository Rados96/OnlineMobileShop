	<queryString>
		<![CDATA[SELECT
     *
FROM
     `order` o INNER JOIN `orderitem` oi ON o.`orderID` = oi.`orderID`
     INNER JOIN `client` c ON o.`clientID` = c.`clientID`
     INNER JOIN `seller` s ON o.`sellerID` = s.`sellerID`
     INNER JOIN `product` p ON oi.`productID` = p.`productID`
WHERE
     o.orderID = $P{orderID}]]>
	</queryString>
